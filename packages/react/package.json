{
  "name": "@squares-app/react",
  "version": "2.5.10",
  "description": "Official React component for squares.vote embeds",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "squares",
    "political",
    "spectrum",
    "react",
    "component",
    "embed"
  ],
  "author": "squares.vote",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seanbhart/squares.git",
    "directory": "packages/react"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}
