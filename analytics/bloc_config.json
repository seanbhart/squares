  "typeDescriptions": {
    "lgmp": "Pragmatic builders who balance individual liberty with global market integration and progressive values.",
    "agmp": "Reform-minded technocrats who combine state oversight with global markets and progressive modernization.",
    "lgmt": "Conventional market builders who emphasize free enterprise while preserving traditional social values.",
    "agmt": "Establishmentarian developers who blend guided markets with institutional authority and traditional norms.",
    "lgsp": "Facilitators of global cooperation who support social programs with civil liberties and international engagement.",
    "agsp": "Planners who manage international redistribution through coordinated state control and global frameworks.",
    "lgst": "Caregivers who balance global engagement with social support and traditional community values.",
    "agst": "Corporatist architects who implement hierarchical social policy through authoritarian global structures.",
    "lnsp": "Organizers of national solidarity who build social programs while protecting democratic freedoms.",
    "ansp": "Collectivists who enforce national redistribution through centralized state authority and unified control.",
    "lnst": "Populists who champion national community solidarity grounded in traditional values and cultural identity.",
    "anst": "Disciplinarians who impose strict national order through state redistribution and traditional social control.",
    "lnmp": "Tradesmen who champion national markets with minimal state interference and free economic exchange.",
    "anmp": "Nationalists who direct market activity through strong state control within closed borders.",
    "lnmt": "Townspeople who preserve local traditions while maintaining community-based market relationships.",
    "anmt": "Sentinels who enforce traditional order through authoritarian control of national markets and society."
  },
